{
  "name": "rayvenues/eloquent-model-generator",
  "description": "Eloquent Model Generator",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "eloquent",
    "model",
    "generator",
    "laravel",
    "php"
  ],
  "autoload": {
    "psr-4": {
      "Ray\\EloquentModelGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1",
    "illuminate/database": "^10.0",
    "illuminate/support": "^10.0",
    "illuminate/config": "^10.0",
    "illuminate/console": "^10.0",
    "doctrine/dbal": "^3.3"
  },
  "require-dev": {
    "pestphp/pest": "^2.6",
    "mockery/mockery": "^1.6"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ray\\EloquentModelGenerator\\Provider\\GeneratorServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "stable"
}

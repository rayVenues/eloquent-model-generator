{
    "name": "rayvenues/eloquent-model-generator",
    "description": "Eloquent Model Generator",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ray\\EloquentModelGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ray\\Tests\\Integration\\": "tests/integration"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/database": "^10.0",
        "illuminate/support": "^10.0",
        "illuminate/config": "^10.0",
        "illuminate/console": "^10.0",
        "doctrine/dbal": "^3.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2",
        "pestphp/pest": "^2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ray\\EloquentModelGenerator\\Provider\\GeneratorServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev"
}
